{
  "log": {
    "loglevel": "debug"
  },
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "network": "tcp,udp",
        "inboundTag": ["DokodemoDoorIn", "SocksIn"],
        "outboundTag": "CdnTlsWsVmessOut"
      }
    ],
    "balancers": []
  },
  "inbounds": [
    {
      "tag": "DokodemoDoorIn",
      "protocol": "dokodemo-door",
      "port": 12345,
      "listen": "0.0.0.0",      
      "settings": {
        "network": "tcp,udp",
        "timeout": 0,
        "followRedirect": true,   // 这里要为 true 才能接受来自 iptables 的流量
        "userLevel": 0
      },
      "streamSettings": {
        
      },     
      "sniffing": {
        "enabled": true,      // 透明代理必须开启
        "destOverride": ["http", "tls"]
      }
    },
    {
      "tag": "SocksIn",
      "protocol": "socks",
      "port": 12346,
      "listen": "127.0.0.1",
      "settings": {
        "auth": "noauth"
      }
    }
  ],
  "outbounds": [
    {
      "tag": "CdnTlsWsVmessOut",
      "protocol": "vmess",
      "sendThrough": "0.0.0.0",     // 主机有多个 ip 时，使用指定 ip 发送数据
      "settings": {
        "vnext": [
          {
            "address": "www.1291114093.ml",
            "port": 443,
